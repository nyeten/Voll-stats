<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Text?>

<Pane xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefWidth="640" prefHeight="480"
      style="-fx-background-color: LIGHTCORAL;">


    <Text fx:id="blocks" text="Block" layoutX="50" layoutY="40" />
    <Button text="Error" layoutX="50" layoutY="90" onAction="#blockError" />
    <Text fx:id="blockErr" text="0" layoutX="75" layoutY="90" />
    <Button text="Solo" layoutX="50" layoutY="140" onAction="#blockSolo" />
    <Text fx:id="blockSol" text="0" layoutX="75" layoutY="140" />
    <Button text="Assist" layoutX="50" layoutY="190" onAction="#blockAssist" />
    <Text fx:id="blockAss" text="0" layoutX="75" layoutY="190" />

    <Text fx:id="attacks" text="Attack" layoutX="140" layoutY="40" />
    <Button text="Error" layoutX="140" layoutY="90" onAction="#attackError" />
    <Text fx:id="attackErr" text="0" layoutX="165" layoutY="90" />
    <Button text="Kill" layoutX="140" layoutY="140" onAction="#attackKill" />
    <Text fx:id="attackKil" text="0" layoutX="165" layoutY="140" />

    <Text fx:id="dig" text="Dig" layoutX="230" layoutY="40" />
    <Button text="Error" layoutX="230" layoutY="90" onAction="#digError" />
    <Text fx:id="digErr" text="0" layoutX="255" layoutY="90" />
    <Button text="Success" layoutX="230" layoutY="140" onAction="#digSuccess" />
    <Text fx:id="digSucc" text="0" layoutX="255" layoutY="140" />

    <Text fx:id="sets" text="Set" layoutX="320" layoutY="40" />
    <Button text="Error" layoutX="320" layoutY="90" onAction="#setError" />
    <Text fx:id="setErr" text="0" layoutX="345" layoutY="90" />
    <Button text="Assist" layoutX="320" layoutY="140" onAction="#setAssist" />
    <Text fx:id="setAss" text="0" layoutX="345" layoutY="140" />

    <Text fx:id="serves" text="Serve" layoutX="410" layoutY="40" />
    <Button text="Error" layoutX="410" layoutY="90" onAction="#serveError" />
    <Text fx:id="serveErr" text="0" layoutX="435" layoutY="90 "/>
    <Button text="Ace" layoutX="410" layoutY="140" onAction="#serveAce" />
    <Text fx:id="serveAce" text="0" layoutX="435" layoutY="140" />
    
    <Text text="Pass" layoutX="500" layoutY="40"/>
    <Button text="Error" layoutX="500" layoutY="90" onAction="#passError" />
    <Text fx:id="passErr" text="0" layoutX="525" layoutY="90"/>
    <Button text="On Target" layoutX="500" layoutY="140" onAction="#passOnTarget"/>
    <Text fx:id="passOnTar" text="0" layoutX="525" layoutY="140" />
    <Button text="Off Target" layoutX="500" layoutY="190" onAction="#passOffTarget" />
    <Text fx:id="passOffTar" text="0" layoutX="525" layoutY="190" />

    <Button text="+1" layoutX="300" layoutY="290" onAction="#plus" />
    <Button text="-1" layoutX="300" layoutY="330" onAction="#minus" />


    <Button text="Back" layoutX="10" layoutY="10" onAction="#back" />

</Pane>


