<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Label?>

<Pane   fx:id = "root" 
        xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      prefWidth="640" prefHeight="480"
      style="-fx-background-color: LIGHTCORAL;">

    <Text text="Teams: " layoutX="150" layoutY="65"/>
    <Text fx:id="curTeam" text="Current Team" layoutX="200" layoutY="65" style="-fx-border-color: black; -fx-padding: 5;" />
    <Button fx:id="next" text='Next' layoutX="400" layoutY="50" onAction="#nextTeam"/>
    <Button fx:id="last" text='Last' layoutX="350" layoutY="50" onAction="#lastTeam"/>
    <Button fx:id="addTeam" text='Add Team' layoutX="450" layoutY="50" onAction="#addTeam"/>
    <Button fx:id="editTeam" text='Edit' layoutX="530" layoutY="50" onAction="#editTeam"/>
    <Button fx:id="delTeam" text='Delete' layoutX="570" layoutY="50" onAction="#deleteTeam"/>





</Pane>